{
  "name": "@adi/adapters",
  "version": "0.1.0",
  "description": "Another DI system for TS/JS (@adapters)",
  "author": "Maciej Urba≈Ñczyk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magicmatatjahu/adi"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ioc",
    "di",
    "dependency injection",
    "inversion of control",
    "js",
    "javascript",
    "ts",
    "typescript",
    "node"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/",
    "./README.md",
    "./LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:prod": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@adi/core": "0.1.0",
    "@adi/common": "0.1.0"
  },
  "peerDependencies": {
    "@angular/core": "^15.2.0",
    "@nestjs/common": "^9.3.9"
  },
  "devDependencies": {
    "@angular/core": "^15.2.0",
    "@nestjs/common": "^9.3.9",
    "jest-preset-angular": "^13.0.0",
    "@ngrx/store": "^15.3.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "jest": "^29.0.3",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
