{
  "name": "@adi/react",
  "version": "0.1.0",
  "description": "Another DI system for TS/JS (@react)",
  "author": "Maciej Urba≈Ñczyk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magicmatatjahu/adi"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ioc",
    "di",
    "dependency injection",
    "inversion of control",
    "js",
    "javascript",
    "ts",
    "typescript",
    "node"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/",
    "./README.md",
    "./LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:prod": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@adi/core": "^0.1.0"
  },
  "devDependencies": {
    "@adi/common": "^0.1.0",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.19",
    "jest": "^26.6.3",
    "react": "^16.13.1",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.5.5",
    "typescript": "^4.4.4"
  }
}
