{
  "name": "@adi/vue",
  "version": "0.1.0",
  "description": "Another DI system for TS/JS (@vue)",
  "author": "Maciej Urba≈Ñczyk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magicmatatjahu/adi"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ioc",
    "di",
    "dependency injection",
    "inversion of control",
    "js",
    "javascript",
    "ts",
    "typescript",
    "vue",
    "vuejs"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/",
    "./README.md",
    "./LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:prod": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@adi/core": "0.1.0",
    "vue-demi": "^0.14.6"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.2",
    "vue": "^2.6.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
